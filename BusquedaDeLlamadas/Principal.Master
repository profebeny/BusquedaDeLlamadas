}

header nav ul {
    list-style: none;
    padding: 0;
    margin: 0;
    display: flex;            
}

header nav ul li {
    flex-grow: 1;    
    border-right: 1.5px solid #fff;                                     
    border-left: 1.5px solid #fff;                                     
}

header nav ul li:hover {
    background: rgba(0, 0, 0, 0.3);
}

header nav ul li a {
    color: #fff;
    display: block;
    margin-top: 12%;
    text-align: center;
    font-size:1.1em;
    color: #fff;
}

header nav ul li a:hover{
    color: #fff;
    text-decoration: none;
}

header nav ul li:hover .children {
    display: block;
    margin-top: 0%;
    width: 15%;
}

header nav ul li .children {
    display: none;
    background: #515A5A;
    position: absolute;
    width: 15%;
    z-index: 1000;              
}

header nav ul li .children li {
    display: block;
    overflow: hidden;            
   width: 100%;
    border-bottom: 3px solid #fff; 
    
}

header nav ul li .children li a {
    display: block;
    color: #fff;
}

header nav ul li .children li a:hover{
